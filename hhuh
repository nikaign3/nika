int main()
{
	vector <string> countries{ "USA","UK","CH","IND","CA","AU" };
	vector <string> code{ "+1","+44", "+86","+91", "+1","+61" };
	string num, cod;
	std::cout <<"1.tel num,2.kods" << endl;
	std::cin >> num >> cod;
	if (code[0] == cod)std::cout << countries[0];
	if (code[1] == cod)std:: cout << countries[1];
	if (code[2] == cod)std::cout << countries[2];
	if (code[3] == cod)std::cout << countries[3];
	if (code[4] == cod)std::cout << countries[4];
	if (code[5] == cod)std::cout << countries[5];
	std::cout << cod << num;
}
