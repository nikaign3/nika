{
	vector<string> v{"usa", "uk","china","india","canada","australia" };
	vector<string>c{"+1","+44","+86","+91","+1","+61" }; 
	int i = 0;
	string atb;
	cin >> atb;
	do {
		if (v[i] == atb) cout << c[i];
		i++;
	} while (i < v.size());


	i = 0;
	cin >> atb;
	do {
		if (c[i] == atb) cout << v[i];
		i++;
	} while (i < c.size());

